                <p>
                    <label for="feedback">Comments and Inquires : </label><br>
                    <textarea name="feedback" cols="60" rows="8"></textarea>
                </p>
                <input type="submit" value="Submit"/>
            </div> 
            <%- include('../../partials/footer'); %>

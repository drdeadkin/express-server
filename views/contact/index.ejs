<body>
    <div id="header">
        <div class="container">
            <%- include('../../partials/header'); %>
            <div id="wrapper"> 
                <h1>Contact me</h1> 
                <form action="mailto:justinilaoo@hotmail.com" ?subject="SignUp" enctype="text/plain" method="post" name="Form">
                <p>
                    <label for="name">Name : </label>
                    <input type="text" name="name" size="40" placeholder="Name" required>
                </p>
                <p>
                    <label for="email">Email : </label>
                    <input type="email" name="email" size="40" placeholder="Email" required>
